      <displayPosition x="539.0" y="89.0"/>
    </node>
    <node id="Multilook">
      <displayPosition x="744.0" y="94.0"/>
    </node>
    <node id="LinearToFromdB">
      <displayPosition x="624.0" y="93.0"/>
    </node>
    <node id="Write">
      <displayPosition x="846.0" y="93.0"/>
    </node>
  </applicationData>
</graph>